<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">	<xs:element name="Address">		<xs:complexType>			<xs:attribute name="clientType" use="optional">				<xs:simpleType>					<xs:restriction base="xs:NMTOKEN">						<xs:enumeration value="Consumer"/>						<xs:enumeration value="Branch"/>						<xs:enumeration value="Retailer"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="addressID" type="xs:string" use="required"/>			<xs:attribute name="addressType" use="required">				<xs:simpleType>					<xs:restriction base="xs:NMTOKEN">						<xs:enumeration value="MainAddress"/>						<xs:enumeration value="BillingAddress"/>						<xs:enumeration value="ShippingAddress"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="clientBranchID" type="xs:string" use="optional"/>			<xs:attribute name="name" type="xs:string" use="required"/>			<xs:attribute name="name2" type="xs:string" use="optional"/>			<xs:attribute name="street" type="xs:string" use="required"/>			<xs:attribute name="zip" type="xs:string" use="required"/>			<xs:attribute name="city" type="xs:string" use="required"/>			<xs:attribute name="isoCountryCode" type="xs:language" use="required"/>			<xs:attribute name="phone" type="xs:string" use="optional"/>			<xs:attribute name="fax" type="xs:string" use="optional"/>			<xs:attribute name="email" type="xs:string" use="optional"/>		</xs:complexType>	</xs:element>	<xs:element name="Addresses">		<xs:complexType>			<xs:sequence>				<xs:element ref="Address" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="Client">		<xs:complexType>			<xs:sequence>				<xs:element ref="Addresses" minOccurs="0"/>			</xs:sequence>			<xs:attribute name="clientID" type="xs:string" use="required"/>			<xs:attribute name="clientName" type="xs:string" use="optional"/>			<xs:attribute name="responseMail" type="xs:string" use="optional"/>		</xs:complexType>	</xs:element>	<xs:element name="Configuration">		<xs:complexType>			<xs:sequence>				<xs:element ref="Feature" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>			<xs:attribute name="quantity" type="xs:positiveInteger" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="Feature">		<xs:complexType>			<xs:attribute name="templateID" type="xs:string" use="required"/>			<xs:attribute name="selectedValue" type="xs:string" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="OrderItem">		<xs:complexType>			<xs:sequence>				<xs:element ref="Configuration"/>			</xs:sequence>			<xs:attribute name="orderItemType" use="optional">				<xs:simpleType>					<xs:restriction base="xs:NMTOKEN">						<xs:enumeration value="standard"/>						<xs:enumeration value="freeOfCharge"/>						<xs:enumeration value="retoure"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="unitOfMeasure" use="optional">				<xs:simpleType>					<xs:restriction base="xs:NMTOKEN">						<xs:enumeration value="pieces"/>						<xs:enumeration value="boxes"/>						<xs:enumeration value="bottles"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="articleID" type="xs:string" use="required"/>			<xs:attribute name="articleName" type="xs:string" use="optional"/>			<xs:attribute name="eanCode" type="xs:string" use="optional"/>			<xs:attribute name="upcCode" type="xs:string" use="optional"/>			<xs:attribute name="clientArticleID" type="xs:string" use="optional"/>			<xs:attribute name="clientArticleName" type="xs:string" use="optional"/>			<xs:attribute name="clientOrderItemID" type="xs:string" use="required"/>			<xs:attribute name="referenceName" type="xs:string" use="optional"/>			<xs:attribute name="addressID" type="xs:string" use="optional"/>			<xs:attribute name="deliveryDate" type="xs:dateTime" use="optional"/>			<xs:attribute name="netPurchasePrice" type="xs:decimal" use="optional"/>			<xs:attribute name="vatRate" type="xs:decimal" use="optional"/>		</xs:complexType>	</xs:element>	<xs:element name="Order">		<xs:complexType>			<xs:sequence>				<xs:element ref="Client"/>				<xs:element ref="OrderItems"/>			</xs:sequence>			<xs:attribute name="schemaMajorVersionID" type="xs:integer" use="required"/>			<xs:attribute name="schemaMinorVersionID" type="xs:integer" use="required"/>			<xs:attribute name="catalogID" type="xs:string" use="required"/>			<xs:attribute name="catalogName" type="xs:string" use="optional"/>			<xs:attribute name="supplierID" type="xs:string" use="required"/>			<xs:attribute name="supplierName" type="xs:string" use="optional"/>			<xs:attribute name="clientOrderID" type="xs:string" use="required"/>			<xs:attribute name="clientSupplierID" type="xs:string" use="optional"/>			<xs:attribute name="generationDate" type="xs:dateTime" use="optional"/>			<xs:attribute name="generatorInfo" type="xs:string" use="optional"/>			<xs:attribute name="currency" type="xs:string" use="optional"/>			<xs:attribute name="vatRate" type="xs:decimal" use="optional"/>		</xs:complexType>	</xs:element>	<xs:element name="OrderItems">		<xs:complexType>			<xs:sequence>				<xs:element ref="OrderItem" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>