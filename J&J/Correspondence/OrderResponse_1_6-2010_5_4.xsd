<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">	<xs:element name="OrderResponse">		<xs:complexType>			<xs:sequence maxOccurs="2">				<xs:element ref="OrderItems" minOccurs="0"/>				<xs:element name="OrderError" minOccurs="0">					<xs:complexType>						<xs:attribute name="errorCode" use="required">							<xs:simpleType>								<xs:restriction base="xs:NMTOKEN">									<xs:enumeration value="1"/>									<xs:enumeration value="2"/>									<xs:enumeration value="3"/>									<xs:enumeration value="4"/>								</xs:restriction>							</xs:simpleType>						</xs:attribute>						<xs:attribute name="errorDescription" type="xs:string"/>					</xs:complexType>				</xs:element>			</xs:sequence>			<xs:attribute name="schemaMajorVersionID" type="xs:integer" use="required"/>			<xs:attribute name="schemaMinorVersionID" type="xs:integer" use="required"/>			<xs:attribute name="orderResponseID" type="xs:string" use="optional"/>			<xs:attribute name="clientOrderID" type="xs:string" use="required"/>			<xs:attribute name="supplierOrderID" type="xs:string" use="optional"/>			<xs:attribute name="orderDate" type="xs:dateTime" use="optional"/>			<xs:attribute name="supplierID" type="xs:string" use="required"/>			<xs:attribute name="supplierName" type="xs:string" use="optional"/>			<xs:attribute name="clientSupplierID" type="xs:string" use="optional"/>			<xs:attribute name="clientID" type="xs:string" use="required"/>			<xs:attribute name="clientName" type="xs:string" use="optional"/>			<xs:attribute name="generationDate" type="xs:dateTime" use="required"/>			<xs:attribute name="generatorInfo" type="xs:string" use="optional"/>			<xs:attribute name="currency" type="xs:string" use="optional"/>		</xs:complexType>	</xs:element>	<xs:element name="OrderItem">		<xs:complexType>			<xs:attribute name="supplierOrderItemID" type="xs:string" use="optional"/>			<xs:attribute name="clientOrderItemID" type="xs:string" use="required"/>			<xs:attribute name="orderItemStatus" use="required">				<xs:simpleType>					<xs:restriction base="xs:NMTOKEN">						<xs:enumeration value="10"/>						<xs:enumeration value="0"/>						<xs:enumeration value="1"/>						<xs:enumeration value="2"/>						<xs:enumeration value="3"/>						<xs:enumeration value="4"/>						<xs:enumeration value="5"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="orderItemType" use="optional">				<xs:simpleType>					<xs:restriction base="xs:NMTOKEN">						<xs:enumeration value="standard"/>						<xs:enumeration value="freeOfCharge"/>						<xs:enumeration value="returns"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="articleID" type="xs:string" use="optional"/>			<xs:attribute name="articleName" type="xs:string" use="optional"/>			<xs:attribute name="eanCode" type="xs:string" use="optional"/>			<xs:attribute name="upcCode" type="xs:string" use="optional"/>			<xs:attribute name="clientArticleID" type="xs:string" use="optional"/>			<xs:attribute name="clientArticleName" type="xs:string" use="optional"/>			<xs:attribute name="deliveryQuantity" type="xs:integer" use="optional"/>			<xs:attribute name="orderedQuantity" type="xs:integer" use="optional"/>			<xs:attribute name="unitOfMeasure" use="optional">				<xs:simpleType>					<xs:restriction base="xs:NMTOKEN">						<xs:enumeration value="pieces"/>						<xs:enumeration value="boxes"/>						<xs:enumeration value="bottles"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="netPurchasePrice" type="xs:decimal" use="optional"/>			<xs:attribute name="orderTransmissionType" use="optional">				<xs:simpleType>					<xs:restriction base="xs:NMTOKEN">						<xs:enumeration value="fax"/>						<xs:enumeration value="phone"/>						<xs:enumeration value="onlineShop"/>						<xs:enumeration value="edi"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="referenceName" type="xs:string" use="optional"/>			<xs:attribute name="errorCode" use="optional">				<xs:simpleType>					<xs:restriction base="xs:NMTOKEN">						<xs:enumeration value="1"/>						<xs:enumeration value="2"/>						<xs:enumeration value="3"/>						<xs:enumeration value="4"/>						<xs:enumeration value="5"/>						<xs:enumeration value="6"/>						<xs:enumeration value="7"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="errorDescription" type="xs:string" use="optional"/>			<xs:attribute name="deliveryDate" type="xs:string" use="optional"/>			<xs:attribute name="shipmentDate" type="xs:dateTime" use="optional"/>			<xs:attribute name="shipmentType" type="xs:string" use="optional"/>		</xs:complexType>	</xs:element>	<xs:element name="OrderItems">		<xs:complexType>			<xs:sequence>				<xs:element ref="OrderItem" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>